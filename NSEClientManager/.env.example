# Supabase Database Connection
DATABASE_URL=postgresql://postgres:[YOUR_PASSWORD]@[YOUR_PROJECT].supabase.co:5432/postgres

# Session Secret (minimum 32 characters)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=your-random-secret-key-change-this-in-production

# Cron Secret for Vercel Cron authentication
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
CRON_SECRET=your-cron-secret-key-change-this-in-production

# Environment
NODE_ENV=production

# Vercel (automatically set by Vercel)
# VERCEL=1
